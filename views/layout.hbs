<!DOCTYPE html>
<html ng-app="banjoman">
<head>
  <meta name="viewport" content="width=device-width">
  <!-- Angular -->
  <script src="../bower_components/angular/angular.min.js"></script>
  <link rel="stylesheet" href="../bower_components/angular/angular-csp.css" media="screen" title="no title" charset="utf-8">
  <!-- ngAudio -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-audio/1.7.2/angular.audio.min.js"></script>
  <!-- createJS -->
  <script src="https://code.createjs.com/easeljs-0.8.2.min.js"></script>
  <!-- ngAnimate -->
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular-animate.js"></script>
  <!-- uiRouter -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.18/angular-ui-router.min.js"></script>
  <!-- jquery -->
  <script src = "../bower_components/jquery/dist/jquery.min.js">
  </script>
  <!--normal-bootstrap -->
  <script src="../bower_components/bootstrap/dist/js/bootstrap.min.js"> </script>
  <link rel="stylesheet" href="../bower_components/bootstrap/dist/css/bootstrap.min.css" media="screen" title="no title" charset="utf-8">
  <!-- Module -->
  <script type="text/javascript" src="../javascripts/module.js"></script>
  <!-- home directive -->
  <script src ="../components/home/home.js"></script>
  <!-- chord directive -->
  <script src ="../components/chords/chords.js"></script>
  <!-- services -->
  <script src ="../services/string_service.js"></script>
  <!-- css -->
  <link rel="stylesheet" href="../stylesheets/style.css" media="screen" title="no title" charset="utf-8">
  <!-- csshake -->
  <link rel="stylesheet" href="../bower_components/csshake/dist/csshake-little.min.css"/>
  <link rel="stylesheet" href="../bower_components/csshake/scss/csshake-little.scss"/>
  <!-- Fonts -->
  <link href='https://fonts.googleapis.com/css?family=Quicksand:400,700|Open+Sans' rel='stylesheet' type='text/css' >
  <link href='https://fonts.googleapis.com/css?family=Amatic+SC:400,700' rel='stylesheet' type='text/css'>
</head>
<body class="col-md-12"
ng-keypress="
$event.which === 59 && play(tuning,chord,'one');
$event.which === 108 && play(tuning,chord,'two');
$event.which === 107 && play(tuning,chord,'three');
$event.which === 106 && play(tuning,chord,'four');
$event.which === 110 && play(tuning,chord,'five');
$event.which === 49 && chordChange('G');
$event.which === 113 && chordChange('Gm');
$event.which ===50 && chordChange('A');
$event.which ===119 &&chordChange('Am');
$event.which ===51 &&chordChange('B');
$event.which ===101 &&chordChange('Bm');
$event.which ===52 &&chordChange('C');
$event.which ===114 &&chordChange('Cm');
$event.which ===53 &&chordChange('D');
$event.which ===116 &&chordChange('Dm');
$event.which ===100 &&chordChange('E');
$event.which ===99 &&chordChange('Em');
$event.which ===102 &&chordChange('F');
$event.which ===118 &&chordChange('Fm');
$event.which ===13 &&strum(tuning,chord,'one','two','three','four');

">
<nav id="header" class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">
        <img id="banjoman"alt="banjoman" ng-src="../images/thebanojman.gif">
      </a>
    </div>

      <span id="title">BANJONEER</span>


    <div id="keybrdBtn"class="navbar-right">
      <button id="innerBtn"type="button" class="btn btn-primary btn-lg" data-toggle="modal" data-target="#myModal">
        How to Play
      </button>
    </div>
  </div>
</nav>
<div class="contianer container-fluid  ">
  <audio-test class=""id="neck"></audio-test>
  <chord-test class="col-md-3" id="fingerings"></chord-test>
  <!-- <ui-view></ui-view> -->
 <span id="selectors">
    <div class="col-md-3">
    <h2>Pick a tuning</h2>
    <select ng-model='tuning' name="tuning" class="form-control selectpicker" id="tuning">
    <option value="GMajor">G major</option>
    <option value="Gm">G minor</option>
    <option value="C">Open C</option>
    <option value="DblC">Double C</option>
    <option value="Gmodal">G modal</option>
    </select>
    <h2>Pick a chord</h2>
    <select ng-change="chordChange(chord)" ng-model="chord" name="chord" id="chord"class="form-control selectpicker">
    <option  ng-value="G">G</option>
    <option  ng-value="Gm">Gm</option>
    <option  ng-value="Gm7">Gm7</option>
    <option  ng-value="A">A</option>
    <option  ng-value="Am">Am</option>
    <option  ng-value="Am7">Am7</option>
    <option  ng-value="B">B</option>
    <option  ng-value="Bm">Bm</option>
    <option  ng-value="Bm7">Bm7</option>
    <option  ng-value="C">C</option>
    <option  ng-value="Cm">Cm</option>
    <option  ng-value="Cm7">Cm7</option>
    <option  ng-value="D">D</option>
    <option  ng-value="Dm">Dm</option>
    <option  ng-value="Dm7">Dm7</option>
    <option  ng-value="E">E</option>
    <option  ng-value="Em">Em</option>
    <option  ng-value="Em7">Em7</option>
    <option  ng-value="F">F</option>
    <option  ng-value="Fm">Fm</option>
    <option  ng-value="Fm7">Fm7</option>
    </select>
    </div>
    </span>


</div>





<!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">

        <h4 class="modal-title" id="myModalLabel">Banjo Controls</h4>
      </div>
      <div class="modal-body">
        <img class="col-md-12" id ="keyboard"ng-src="../images/keyboard.png" alt="keyboard" />

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>












</body>

</html>
